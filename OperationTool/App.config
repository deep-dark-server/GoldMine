<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
    <sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" >
      <section name="GoldMine.OperationTool.DBConnect" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
    </sectionGroup>
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>
  <log4net>
    <appender name="ErrorRollingFile" type="log4net.Appender.RollingFileAppender">
      <file value="Error.log" />
      <appendToFile value="true" />
      <maximumFileSize value="100KB" />
      <maxSizeRollBackups value="2" />

      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%level %thread %logger - %message%newline" />
      </layout>

    </appender>
    <root>
      <level value="ERROR" />
      <appender-ref ref="ErrorRollingFile" />
    </root>
  </log4net>
  <appSettings>
      <add key="AWSProfileName" value=""/>
      <add key="AWSRegion" value="us-west-2" />
  </appSettings>
  <applicationSettings>
    <GoldMine.OperationTool.DBConnect>
      <setting name="UseLocalDB" serializeAs="String">
        <value>True</value>
      </setting>
      <setting name="LocalDBAddress" serializeAs="String">
        <value />
      </setting>
    </GoldMine.OperationTool.DBConnect>
  </applicationSettings>
</configuration>